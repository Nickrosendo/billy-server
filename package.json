{
  "name": "billy-server",
  "version": "1.0.0",
  "description": "NodeJs server of billy app",
  "main": "index.js",
  "engines": { "node": "10.24.1" },
  "scripts": {
    "dev": "npm run build && npm start",
    "build": "babel server --out-dir dist && babel server --out-dir functions",
    "start": "node dist/index.js",
    "test": "jest",
    "test-coverage": "jest --collectCoverage",
    "firebase": "firebase"
  },
  "pre-commit": [
    "build"
  ],
  "keywords": [
    "node"
  ],
  "author": "Nicolas Rosendo",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "firebase-functions": "^2.1.0",
    "firebase-tools": "^6.2.2",
    "mongoose": "5.3.0",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "axios": "^0.18.0",
    "firebase-admin": "^6.4.0",
    "jest": "23.6.0",
    "nock": "10.0.0",
    "nodemon": "1.18.4",
    "pre-commit": "^1.2.2"
  }
}
